{
  "python.formatting.provider": "yapf",
  "editor.tabSize": 2,
  "jupyter.jupyterServerType": "local",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/__pycache__": true,
    "**/.vscode": true,
    "*.egg-info": true
  },
  "explorerExclude.backup": null,
  "editor.formatOnSave": true
}
